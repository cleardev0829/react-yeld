{
    "name": "spacepup-frontend",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@testing-library/jest-dom": "^5.12.0",
        "@testing-library/react": "^11.2.6",
        "@testing-library/user-event": "^12.8.3",
        "@types/jest": "^26.0.23",
        "@types/node": "^12.20.10",
        "@types/react": "^17.0.4",
        "@types/react-dom": "^17.0.3",
        "axios": "^0.21.1",
        "bootstrap": "^4.5.3",
        "moment": "2.29.1",
        "moment-timezone": "^0.5.33",
        "node-sass": "6.0.0",
        "nouislider": "15.1.0",
        "react": "^17.0.2",
        "react-bootstrap": "^1.4.3",
        "react-bootstrap-switch": "15.5.3",
        "react-chrono": "^1.10.1",
        "react-datetime": "3.0.4",
        "react-device-detect": "^1.17.0",
        "react-dom": "^17.0.2",
        "react-router": "5.2.0",
        "react-router-dom": "5.2.0",
        "react-scripts": "4.0.3",
        "react-select": "4.3.1",
        "react-tagsinput": "3.19.0",
        "react-tsparticles": "^1.27.0",
        "reactstrap": "8.9.0",
        "rellax": "1.12.1",
        "source-map-explorer": "^2.5.2",
        "styled-components": "^5.3.1",
        "tsparticles": "^1.27.0",
        "typescript": "^4.2.4",
        "web-vitals": "^1.1.1"
    },
    "scripts": {
        "dev": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "build:analyze": "npm run build && npm run analyze",
        "analyze": "source-map-explorer 'build/static/js/*.js'",
        "format": "npx prettier --write .",
        "compile-sass": "node-sass src/assets/scss/now-ui-kit.scss src/assets/css/now-ui-kit.css",
        "minify-sass": "node-sass src/assets/scss/now-ui-kit.scss src/assets/css/now-ui-kit.min.css --output-style compressed",
        "map-sass": "node-sass src/assets/scss/now-ui-kit.scss src/assets/css/now-ui-kit.css --source-map true"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "dotenv": "^8.2.0",
        "identity-obj-proxy": "^3.0.0",
        "nock": "^13.0.6"
    },
    "jest": {
        "moduleNameMapper": {
            "\\.(scss|sass|css)$": "identity-obj-proxy"
        }
    }
}
